<%= form_for @dish_attachment, :html => { :class => "form-horizontal dish_attachment" } do |f| %>

  <% if @dish_attachment.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@dish_attachment.errors.count, "error") %> prohibited this dish_attachment from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @dish_attachment.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :dish_id, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :dish_id, :class => 'form-control' %>
    </div>
    <%=f.error_span(:dish_id) %>
  </div>
<!--  <div class="form-group">
    <%= f.label :image, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :image, :class => 'form-control' %>
    </div>
    <%=f.error_span(:image) %>
  </div> -->
  <div class="form-group">
    <%= f.label :images, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :images, :class => 'form-control' %>
    </div>
    <%=f.error_span(:images) %>
  </div>

  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                dish_attachments_path, :class => 'btn btn-default' %>
    </div>
  </div>

<% end %>
